{
    "functions": {
        "api/lambda.php": {
            "runtime": "vercel-php@0.7.4"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "public/build/$1"
        },
        {
            "src": "/(css|js|images)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "api/lambda.php"
        }
    ],
    "outputDirectory": "public"
}
